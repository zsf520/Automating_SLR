doi = {10.1145/3495018.3495290},
abstract = {Tennis is a unique and beautiful sport. In recent years, tennis has become more and more enthusiastic around the world, and some tennis academies have sprung up like bamboo shoots. With the gradual improvement of the tennis atmosphere, the tennis crowd has gradually increased, but at this time there is a lack of a comprehensive tennis training base and a scientific and systematic talent training model to promote the development of Chinese tennis in the future. And it happens to be a period of rapid development of artificial intelligence. Therefore, this article studies and discusses the training path of tennis talents in colleges and universities based on artificial intelligence. Through an in-depth analysis of the training path of our country's tennis competitive talents, the current stage of our country's tennis talent training mainly relies on the following three methods: sports team training model; club training model; government, enterprise, and family joint training model. Through the introduction of artificial intelligence-related technologies, experiments on these three methods are conducted in the artificial intelligence environment. The results show that, compared with ordinary tennis talent training models, the physical and humanistic qualities of college tennis talents get 30%. The improvement of the service period and technical level of college tennis competitive talents has also reached 10% compared with the traditional training path. This shows that in the current global environment, combining the training path of tennis competitive talents in colleges and universities with the emerging artificial intelligence technology, the effect is very significant, and it is the development trend of future tennis competitive talent training.},
booktitle = {2021 3rd International Conference on Artificial Intelligence and Advanced Manufacture},
pages = {854–858},
numpages = {5},
location = {Manchester, United Kingdom},
series = {AIAM2021}
