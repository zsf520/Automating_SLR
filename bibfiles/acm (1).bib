@article{10.1145/3440759,
author = {Bowman, Nicholas A. and Jarratt, Lindsay and Culver, KC and Segre, Alberto M.},
title = {The Impact of Pair Programming on College Students’ Interest, Perceptions, and Achievement in Computer Science},
year = {2021},
issue_date = {September 2021},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {21},
number = {3},
url = {https://doi.org/10.1145/3440759},
doi = {10.1145/3440759},
abstract = {Active and collaborative learning has shown considerable promise for improving student outcomes and reducing group disparities. As one common form of collaborative learning, pair programming is an adapted work practice implemented widely in higher education computing programs. In the classroom setting, it typically involves two computer science students working together on the same programming assignment. The present study examined a cluster-randomized trial of 1,198 undergraduates in 96 lab sections. Overall, pair programming had no significant effect on students’ course performance; subject matter interest; plans for future coursework; or their confidence, comfort, and anxiety with computer science. These findings were consistent across various student characteristics, except that students with favorable pretest scores exhibited negative effects from pair programming.},
journal = {ACM Trans. Comput. Educ.},
month = {may},
articleno = {19},
numpages = {19},
keywords = {experimental design, college students, collaborative learning, Pair programming}
}

@inproceedings{10.1145/3411763.3457780,
author = {E. Gilbert, Juan},
title = {Equitable AI},
year = {2021},
isbn = {9781450380959},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3411763.3457780},
doi = {10.1145/3411763.3457780},
abstract = {Fairness in machine learning (ML) and artificial intelligence (AI) has gained much attention in recent years, which has led to the creation of multiple fairness metrics and frameworks. However, it is still challenging for practitioners to decide which fairness metrics to incorporate in their work and how to effectively incorporate fairness into ML and AI models and systems. ML and AI can benefit from computer-human interaction (CHI) expertise from our backgrounds in empirically grounded research. ML and AI black-box models that are creating biased results are in need of CHI expertise to add the human aspect to the evaluation and creation of AI systems. This will result in what I call Equitable AI systems. In my talk, I will describe an equitable AI system I created to create holistic diversity in higher education admissions, scholarships, hiring, etc. I will also discuss research from a CHI perspective in addressing bias in ML and AI.},
booktitle = {Extended Abstracts of the 2021 CHI Conference on Human Factors in Computing Systems},
articleno = {5},
numpages = {2},
keywords = {Human-Centered AI, Equitable AI, Bias in AI},
location = {Yokohama, Japan},
series = {CHI EA '21}
}

@inproceedings{10.1145/3434780.3436652,
