doi = {10.1145/3495018.3495385},
abstract = {Smart classrooms aim to cultivate smart talents with high-level cognitive abilities and creativity. Research on the status quo, hotspots and development trends of smart classrooms can better explore, construct and develop smart classrooms. Using the literature analysis visualization software Citespace, the Chinese literature in the CNKI database was used as the research object, and the subject term "smart classroom" was used to search, and the literature of the core journals from 2006 to 2020 was analyzed through the bibliometric method and visual atlas Exploring information such as annual publication trends, research institutions, research authors, research frontiers, research hotspots and other information found that domestic research hotspots for smart classrooms mainly focus on "artificial intelligence", "smart education", "teaching mode", and "deep learning" And "learning architecture" several topics. Research institutions are mainly distributed in East China Normal University, Northeast Normal University, and the FLYTEK Big Data Research Institute. Under the guidance of the learning architecture, with the help of artificial intelligence and big data analysis technology, smart classroom teaching can be better carried out. In the future, with the empowerment of artificial intelligence technology, deep and flexible learning in various specific disciplines will be the further development trend of smart classrooms in the future.},
booktitle = {2021 3rd International Conference on Artificial Intelligence and Advanced Manufacture},
pages = {1295–1299},
numpages = {5},
location = {Manchester, United Kingdom},
series = {AIAM2021}
}

