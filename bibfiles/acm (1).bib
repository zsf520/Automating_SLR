series = {ICEGOV '21}
}

@inproceedings{10.1145/3412841.3441993,
author = {Suo, Kun and Shi, Yong and Hung, Chih-Cheng and Bobbie, Patrick},
title = {Quantifying Context Switch Overhead of Artificial Intelligence Workloads on the Cloud and Edges},
year = {2021},
isbn = {9781450381048},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3412841.3441993},
doi = {10.1145/3412841.3441993},
abstract = {Context switching is the fundamental technique for providing flexible and efficient utilization of CPU resources in the multitasking system. However, context switching also introduces non-trivial overhead due to its complicated activities, resulting in not only significant performance degradation of applications, but also dramatic system low efficiency. In this paper, we perform a comprehensive and empirical study on the performance and overhead of context switches in modern artificial intelligence workloads, which identifies unrevealed and important impact on cloud servers and edge/IoT devices. Our observations and root cause analysis cast light on optimizing the system stack of modern operating systems to more efficiently support artificial intelligence workloads on the cloud and edge systems.},
booktitle = {Proceedings of the 36th Annual ACM Symposium on Applied Computing},
pages = {1182–1189},
numpages = {8},
keywords = {context switch, edge, system, artificial intelligence, cloud},
location = {Virtual Event, Republic of Korea},
series = {SAC '21}
}

@inproceedings{10.1145/3557738.3557848,
author = {Pandyaswargo, Andante Hadi and Maghfiroh, Meilinda Fitriani Nur and Onoda, Hiroshi},
title = {Readiness Status of Artificial Intelligence Applications on Electric Vehicles: A Mini Global Review and Analysis Using the J-TRA Method},
year = {2022},
isbn = {9781450397186},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3557738.3557848},
doi = {10.1145/3557738.3557848},
abstract = {The transportation sector is a significant contributor to global greenhouse gas (GHG) emissions. It is estimated that replacing fossil fuel-based vehicles with electric vehicles (EVs) powered by sustainable and renewable energy could contribute to approximately 21% of emission avoidance by 2050. To improve the efficiency of EV operation, various artificial intelligence (AI) technologies have been applied. Examples include charging system optimization, self-driving car technology, and traffic control technology. To understand the current readiness status of those technologies applications, a small database of AI use in EVs that is in practice globally is constructed. There are 23 locations of prototype projects identified. The projects are categorized by the AI type, developer type, size of operation, and readiness status. Readiness status is analysed with the Japan Technology Readiness Assessment (J-TRA) methodology. There are seven analysed parameters: 1) Market, 2) Technology development, 3) System Integration, 4) Sustainability Verification, 5) Safety, 6) Commercialization and 7) Cost and Risk. The results show that while there is a promising market, steady progress in technological development, and verified environmental benefits, more work is needed to ensure safety and integration with the current systems before the technology can reach higher readiness levels of commercialization, cost, and risk-coping mechanisms.},
booktitle = {Proceedings of the 2022 International Conference on Engineering and Information Technology for Sustainable Industry},
articleno = {26},
numpages = {5},
keywords = {smart grid, TRA, self-driving car, electric vehicle, Artificial intelligence},
location = {Alam Sutera, Tangerang, Indonesia},
series = {ICONETSI '22}
}

@inproceedings{10.1145/3286606.3286784,
author = {El Mhouti, Abderrahim and Erradi, Mohamed},
title = {Towards a Smart Learning Management System (Smart-LMS) to Improve Collaborative Learning in Higher Education},
year = {2018},
isbn = {9781450365628},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3286606.3286784},
doi = {10.1145/3286606.3286784},
abstract = {With the increasing number of higher education students using e-learning technology, collaborative learning has been suggested as a solution to the lack of sufficient support to learners in e-learning environments, particularly LMS (Learning Management System). Nevertheless, current collaborative learning puts a heavy emphasis on group discussion, but provides inadequate support for individual learners to promote teamwork during the whole learning process.The aim of this paper is to propose an intelligent LMS able to improve student's collaboration practices in higher education. The system provides teachers information about the levels of collaboration and productivity of each student and group; and as a result, teachers exploit data provided to promote collaborative learning, avoid students' isolation and motivate them to use collaboration tools. The intelligent LMS called "smart-Cloud Collaborative LMS" integrates a Multi-Agents System (MAS) responsible to the tracking of collaboration and productivity levels of each student and group. On the other hand, in order to optimize the large-scale resource management, the proposed smart-Cloud Collaborative LMS is based on a cloud computing architecture.},
booktitle = {Proceedings of the 3rd International Conference on Smart City Applications},
articleno = {7},
numpages = {9},
keywords = {cloud computing, smart-Cloud Collaborative LMS, Multi-Agents System, collaborative learning, E-learning},
location = {Tetouan, Morocco},
series = {SCA '18}
}

@inproceedings{10.1145/3450148.3450151,
author = {Wang, Shan},
title = {Innovative Thinking and Practice of Mobile Interaction Design Teaching in Artificial Intelligence Era},
year = {2021},
isbn = {9781450388542},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3450148.3450151},
doi = {10.1145/3450148.3450151},
abstract = {In recent years, the rapid development of artificial intelligence opens a new era and has had a profound influence on the application scenarios, design objects and design methods of interaction design. The intelligent development of mobile applications has become the focus of Internet companies, brings a huge demand for crossover and diversified interaction design talents. For university teachers, it is a task ahead to cultivate high-quality design talents who can adapt to the changing needs of the times. In this context, this paper analyzes the new connotation of interaction design in intelligent era and the new requirements for the knowledge structure and innovative ability of design talents. Based on this, we present new teaching concept for Mobile Interaction Design course and a complete teaching reform plan of optimizing and expanding teaching content modules, adopting thematic task-driven and enterprise-participated teaching mode, which we explore and practice in our course teaching and achieve good results.},
booktitle = {Proceedings of the 2021 12th International Conference on E-Education, E-Business, E-Management, and E-Learning},
