@inproceedings{10.1145/3444465.3444474,
author = {Griazev, Mikhail and Sabinina, Anna and Sycheva, Irina and Izmalkova, Svetlana and Sycheva, Natalia},
title = {Actualization of Environmental Education as an Innovative and Digital Resource for Sustainable Regional Development},
year = {2021},
isbn = {9781450388313},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3444465.3444474},
doi = {10.1145/3444465.3444474},
abstract = {The scientific and expert community does not fully explore such a factor of sustainable regional development as actualization of environmental education. The goal of this research is to develop a list of prospective majors in the higher education system for solving current ecological challenges ensuring stable regional development. We have made a predictive assessment of the demand for prospective majors in environmental science, which is not limited to the study of the need for such personnel strictly in the labor market. A number of factors that influence people's preferences when choosing promising areas of environmental education was taken into account, such as: the industry development in the field of renewable energy; the worldwide search for a waste-free model of plastic recycling; the global scientific, technological and innovative character of regional economy development, etc. The results of this study are based on expert opinions from the environmentally-oriented scientific community and employers in the Tula region, who took part in the online survey through questionnaires. The opinion of rectors of leading universities, as well as their staff and students, on the functioning of the higher education system in the area of environmental science considering its actualization was of no less importance. The results obtained include a list of recommended majors in the field of environmental science, capable of utilizing their digital competencies and skills in solving current environmental problems; trending activities in the field of environmental development and protection, which will be in demand in the economy of the future for the sustainable development of Russian regions.},
booktitle = {Proceedings of the 2nd International Scientific Conference on Innovations in Digital Economy},
articleno = {11},
numpages = {7},
keywords = {Region, Environmental education, Sustainable development, Environmental problems, Innovative and digital resource, Actualization},
location = {Saint, Petersburg, Russian Federation},
series = {SPBPU IDE '20}
}

@inproceedings{10.1145/3410886.3410973,
author = {Ajoodha, Ritesh and Jadhav, Ashwini and Dukhan, Shalini},
title = {Forecasting Learner Attrition for Student Success at a South African University},
year = {2020},
isbn = {9781450388474},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3410886.3410973},
doi = {10.1145/3410886.3410973},
abstract = {In this paper we attempt to deduce student attrition at a South African higher-education institution with the aim of identifying students who are likely to be in need of academic support so that a focus could be provided on improving their academic performance. The significance of this paper is on using computer science and information technology to address learner attrition (an African reality) and thereby impact the low university throughput and retention rates positively. We trained several machine learning classification models to deduce the student into four risk classes using only Grade 12 marks and background characteristics of the learner. We provide the following contributions: (a) the first known published trained classifier able to calculate the distribution over a students’ risk profile for a South African university focused on the conceptual framework; (b) a ranking of employed features according to their entropy to correctly classify the class variable; (c) a comparison of trained classifiers able to calculate the probability of a students’ risk profile for a South African higher-education research-intensive university; and (d) an interactive program which is able to calculate the posterior probability over the student’s risk profile so that support can be provided to them. The random forest classification model achieves the best performance with a 82% accuracy over these four risk profiles. We argue for introducing predictive tools to enhance student success and student support initiatives in Higher-Education Institutions. This work will benefit academic developers and staff who provide support to students who are at academic risk of completing their undergraduate Science programmes.},
booktitle = {Conference of the South African Institute of Computer Scientists and Information Technologists 2020},
pages = {19–28},
numpages = {10},
keywords = {Pre-college or schooling, South Africa, Biographical characteristics, Identifying at-risk biographical profiles, Background features, Individual attributes, Bayesian analysis, Model-based machine learning},
location = {Cape Town, South Africa},
series = {SAICSIT '20}
}

