@inproceedings{10.1145/3080845.3080866,
author = {Chen, Shih-Hsun and Fu, Tzu-Keng},
title = {Eliminating Artificial-Natural Dichotomy A Formal Study on a Core Cognitive Process in Artificial Intelligence},
year = {2017},
isbn = {9781450352314},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3080845.3080866},
doi = {10.1145/3080845.3080866},
abstract = {The core of this paper is to provide a framework for analyzing the naturalness and artificiality in terms of the new rising of square of opposition, which have fruitful results in doing conceptual analysis in many different disciplines. In this framework, we can deal with two perspectives of analyzing the concepts of naturalness and artificiality: (1) when we focus on intension perspective, the logical relations of natural- ness and artificiality by using the formal square of opposition with na\"{\i}ve set theory, the trivialization of the dichotomy of artificiality and naturalness in artificial intelligence will be made; (2) when we focus on extension perspective, with the concept of being natural in tandem with the concept of being artificial, we find that we need to elaborate the inherent inter- inclusive relationship between artificiality and naturalness, either by logical hexagon or by the SQ-NAT-ART with the K-transformation that we define, and a paradigm shift from GOFAI to emergentism AI occurs. On the above basis, we suggest that SQ-NAT-ART with the K-transformation will be a good preparatory work for the research of naturalness and artificiality.},
booktitle = {Proceedings of the 2017 International Conference on Artificial Intelligence, Automation and Control Technologies},
articleno = {21},
numpages = {6},
keywords = {Artificial Intelligence, Emer-gentism, Square of Opposition, Logical Hexagon, Aristotle's Logic},
location = {Wuhan, China},
series = {AIACT '17}
}

@inproceedings{10.1145/3569219.3569418,
author = {Ghajargar, Maliheh and Bardzell, Jeffrey and Lagerkvist, Love},
title = {A Redhead Walks into a Bar: Experiences of Writing Fiction with Artificial Intelligence},
year = {2022},
isbn = {9781450399555},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3569219.3569418},
doi = {10.1145/3569219.3569418},
abstract = {Human creativity has been often aided and supported by artificial tools, spanning traditional tools such as ideation cards, pens, and paper, to computed and software. Tools for creativity are increasingly using artificial intelligence to not only support the creative process, but also to act upon the creation with a higher level of agency. This paper focuses on writing fiction as a creative activity and explores human-AI co-writing through a research product, which employs a natural language processing model, the Generative Pre-trained Transformer 3 (GPT-3), to assist the co-authoring of narrative fiction. We report on two progressive – not comparative – autoethnographic studies to attain our own creative practices in light of our engagement with the research product: (1) a co-writing activity initiated by basic textual prompts using basic elements of narrative and (2) a co-writing activity initiated by more advanced textual prompts using elements of narrative, including dialects and metaphors undertaken by one of the authors of this paper who has doctoral training in literature. In both studies, we quickly came up against the limitations of the system; then, we repositioned our goals and practices to maximize our chances of success. As a result, we discovered not only limitations but also hidden capabilities, which not only altered our creative practices and outcomes, but which began to change the ways we were relating to the AI as collaborator.},
booktitle = {Proceedings of the 25th International Academic Mindtrek Conference},
pages = {230–241},
numpages = {12},
keywords = {Storytelling, Creative writing, Artificial creativity, GPT-3, Creativity tool, AI co-creativity},
location = {Tampere, Finland},
series = {Academic Mindtrek '22}
}

