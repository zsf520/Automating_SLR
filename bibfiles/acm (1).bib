abstract = {In recent years, with the vigorous development of China's economy, society and culture, ideological and political education and mental health education in colleges and universities are facing new challenges. Simple ideological and political education is no longer suitable for the development of young people. In the reality of education, due to the lack of a comprehensive understanding of ideological and political work, and do not have the relevant psychological knowledge, many educators treat all problems, especially psychological problems as ideological and political problems, which will have a negative impact on the growth of young people. Therefore, we need to strengthen the effectiveness of ideological and political education, make ideological and political education more scientific, predictable and targeted. The purpose of this paper is to study the application of psychology in higher education. This paper first expounds the concept of psychology, and then introduces the application value of psychology in college education and teaching. This paper takes teachers from four colleges and universities in Sichuan province as the research object, uses questionnaire survey method, and makes statistics on 178 questionnaires. The analysis results show that 62% of the teachers realize the importance of psychology to education work.},
booktitle = {2021 2nd International Conference on Computers, Information Processing and Advanced Education},
pages = {515–518},
numpages = {4},
location = {Ottawa, ON, Canada},
series = {CIPAE 2021}
}

