}

@inproceedings{10.1145/3341525.3387381,
author = {Kalra, Shekhar and Thevathayan, Charles and Hamilton, Margaret},
title = {Developing Industry-Relevant Higher Order Thinking Skills in Computing Students},
year = {2020},
isbn = {9781450368742},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3341525.3387381},
doi = {10.1145/3341525.3387381},
abstract = {A 2016 survey commissioned by the British Higher Education Council revealed CS graduates had one of the highest unemployment rates. Industry sources attribute this to a growing gap between employer expectations and university curriculum. This gap primarily consists of soft skills and higher-order thinking skills, which include the ability to analyse, synthesise, evaluate, critique and design. Technical job advertisements increasingly specify higher order thinking skills as entry requirements for graduates.This paper presents the results of our work studying whether formative tasks can develop higher-order thinking skills. The formative tasks consisted of quizzes covering the first four levels of the Bloom's taxonomy. The higher-order thinking skills were measured based on the static and dynamic models students created for a real world problem, as part of the final exam. Our results reveal a strong correlation exists between application and analysis level quizzes and design activities needing higher-order thinking in the exam. Students repeating the quizzes several times also had better results than others in higher-order design skills. These results suggest that practising with well-constructed quizzes especially at the higher Bloom's level exercises the cognitive channels that help improve higher-order thinking skills.},
booktitle = {Proceedings of the 2020 ACM Conference on Innovation and Technology in Computer Science Education},
pages = {294–299},
numpages = {6},
keywords = {higher order thinking skills, computer science education, intelligent tutoring systems, employability},
location = {Trondheim, Norway},
series = {ITiCSE '20}
}

@inproceedings{10.1145/3482632.3483015,
author = {Su, Zheqian and Liu, Miao and Song, Bailin and Lian, Shulin},
title = {The Intervention of Internet Technology on Students' English Learning in the Intelligent Era},
year = {2021},
isbn = {9781450390255},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3482632.3483015},
doi = {10.1145/3482632.3483015},
abstract = {Objective: With the development of the Internet, Internet technology has had a great impact on English learning. This article explores the general expression of the performance, motivation and anxiety of students in Chinese medicine undergraduates in second language learning (SLL). performance, motivation, and anxiety for Chinese Medicine undergraduates in China, it is to analyze the attribution and relationship of above three aspects, and propose the method of using artificial intelligence to improve students’ learning motivation, reduce learning anxiety and improve learning ability. Methods: it is to select the undergraduate students (n = 300) in Jilin Province of China as the subjects. Use the Chinese version of the Foreign Language Learning Anxiety Scale under Internet technology, the Chinese Undergraduate English Learning Motivation Questionnaire, the self-made College Students’ English learning ability evaluation scale and the English Writing Scale were used for test. Results: SLL performance, motivation, and anxiety for Chinese Medicine undergraduates are in the middle level among all undergraduates in China, and their English writing ability is lower than the middle level; Analysis of variance shows that there are significant differences in gender (p&lt;0.01) and grade (p&lt;0.05); learning ability in grade has extremely significant difference (p&lt;0.01); female 's learning ability and writing ability were significantly higher than male's (p&lt;0.01);Demographic variables were not statistically correlated with SLL performance, motivation, and anxiety (p&gt;0.05); SLL anxiety had remarkable correlation with learning motivation (p&gt;0.05); learning anxiety was statistically correlated with learning ability (p&lt;0.01); learning motivation was negative correlative with learning ability (P&lt;0.01). Regression analysis showed that learning anxiety and motivation had 44.3% and 5.8% explanatory power to English learning ability respectively. Intervention experiment results showed that Internet learning system significantly improved the performance of the experimental group (P&lt;0.01), and the performance of the experimental group in listening, reading, writing and translation was significantly higher than that of the control group (p&lt;0.01). the effectiveness of Internet learning mode had remarkable negative relationship with students’ learning anxiety (P&lt;0.01), and was significantly positive with learning motivation and learning ability (P &lt; 0.01). Conclusion: Based on the data-driven theory, the empirical study of Internet learning system in English teaching is completed, which provides an important theoretical support for the current College English intelligent teaching in China, which also provides a strong argument for the “scaffolding function” of AI in language learning. At the same time, the research also puts forward innovative ways to improve the effect of College English teaching from the perspective of psychology: through second language learning with AI resources, the “challenging task” set by the intelligent learning system increases the intensity and difficulty of learning, thus arousing the “promoting anxiety” of second language learners, and further internalizing the external and instrumental motivation in depth in order to improve English learning ability.},
booktitle = {2021 4th International Conference on Information Systems and Computer Aided Education},
pages = {775–780},
numpages = {6},
location = {Dalian, China},
series = {ICISCAE 2021}
}

