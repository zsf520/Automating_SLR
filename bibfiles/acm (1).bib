@inproceedings{10.1145/3495018.3495342,
author = {Lin, Lin},
title = {Investigation and Research on the Cultivation of Students' English Interests under Artificial Intelligence Technology},
year = {2022},
isbn = {9781450385046},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3495018.3495342},
doi = {10.1145/3495018.3495342},
abstract = {With the vigorous development of artificial intelligence, there are abundant types of learning resources and various learning methods. The use of digital resources in the context of the Internet in learning can stimulate students’ interest in English learning, improve their English listening, speaking, reading, and writing skills, and effectively improve the efficiency of junior high school English classroom learning. Different people have different learning effects in the same learning environment because their own qualities are certainly one factor, and the more important factor is the different attitudes of the learners, that is, whether the learners are interested in learning. Interest can arouse students’ curiosity and eager thirst for knowledge, and interest can promote students’ active learning, spontaneous learning, and diligent learning more than intelligence and skills. For the English learning of junior high school students, interest is the first condition for them to learn English well. Only when students are interested in English learning, can they actively participate in English learning, and gradually become more and more interested in the specific English learning process. Through the questionnaire survey of middle school students, we can understand their tendency to attribute to English learning performance in the process of English learning. This paper aims to investigate whether middle school students are more interested in English, the higher their English scores will be, and whether male and female students are different in their interest in English. In this way, in the process of English teaching, attribution theory can be used to combine students' reality and their own experience to guide and train students in attribution so that they can form correct attributions. At the same time, this paper has a certain guiding role for workers engaged in English education for middle school students. It can help them understand the attribution tendency of middle school students in the process of English learning and give middle school students correct attribution guidance and training, thus improving teachers’ English teaching performance.},
booktitle = {2021 3rd International Conference on Artificial Intelligence and Advanced Manufacture},
pages = {1091–1096},
numpages = {6},
location = {Manchester, United Kingdom},
series = {AIAM2021}
}

@inproceedings{10.1145/3556089.3556102,
author = {Hanne, Thomas and Gachnang, Phillip and Gatziu Grivas, Stella and Kirecci, Ilyas and Schmitter, Paul},
title = {Artificial Intelligence and Machine Learning for Maturity Evaluation and Model Validation},
year = {2022},
isbn = {9781450396394},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3556089.3556102},
doi = {10.1145/3556089.3556102},
abstract = {In this paper, we discuss the possibility of using machine learning (ML) to specify and validate maturity models, in particular maturity models related to the assessment of digital capabilities of an organization. Over the last decade, a rather large number of maturity models have been suggested for different aspects (such as type of technology or considered processes) and in relation to different industries. Usually, these models are based on a number of assumptions such as the data used for the assessment, the mathematical formulation of the model and various parameters such as weights or importance indicators. Empirical evidence for such assumptions is usually lacking. We investigate the potential of using data from assessments over time and for similar institutions for the ML of respective models. Related concepts are worked out in some details and for some types of maturity assessment models, a possible application of the concept is discussed.},
booktitle = {2022 13th International Conference on E-Business, Management and Economics},
pages = {256–260},
numpages = {5},
location = {Beijing, China},
series = {ICEME 2022}
}

