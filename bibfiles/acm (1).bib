@article{10.1145/3470745,
author = {Moon, Brian and Bayat, Farima Fatahi and Nair, Sneha and Slaughter, Andrew},
title = {Challenges for Introducing Artificial Intelligence to Improve the Efficiency of a next Generation Assessment Approach},
year = {2021},
issue_date = {September 2021},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {2021},
