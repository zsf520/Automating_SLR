@inproceedings{10.1145/3514094.3534139,
author = {Liu, Yuxin and Moore, Adam and Webb, Jamie and Vallor, Shannon},
title = {Artificial Moral Advisors: A New Perspective from Moral Psychology},
year = {2022},
isbn = {9781450392471},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3514094.3534139},
doi = {10.1145/3514094.3534139},
abstract = {Philosophers have recently put forward the possibility of achieving moral enhancement through artificial intelligence (e.g., Giubilini and Savulescu's version [32]), proposing various forms of "artificial moral advisor" (AMA) to help people make moral decisions without the drawbacks of human cognitive limitations. In this paper, we provide a new perspective on the AMA, drawing on empirical evidence from moral psychology to point out several challenges to these proposals that have been largely neglected by AI ethicists. In particular, we suggest that the AMA at its current conception is fundamentally misaligned with human moral psychology - it incorrectly assumes a static moral values framework underpinning the AMA's attunement to individual users, and people's reactions and subsequent (in)actions in response to the AMA suggestions will likely diverge substantially from expectations. As such, we note the necessity for a coherent understanding of human moral psychology in the future development of AMAs.},
booktitle = {Proceedings of the 2022 AAAI/ACM Conference on AI, Ethics, and Society},
pages = {436–445},
numpages = {10},
keywords = {ai moral enhancement, artificial moral advisor, moral psychology, normative ethics, ai ethics},
location = {Oxford, United Kingdom},
series = {AIES '22}
}

@inproceedings{10.1145/3486011.3486503,
author = {Fake, Helen and Dabbagh, Nada},
title = {The Personalized Learning Interaction Framework: Expert Perspectives on How to Apply Dimensions of Personalized Learning to Workforce Training and Development Programs},
year = {2021},
isbn = {9781450390668},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3486011.3486503},
doi = {10.1145/3486011.3486503},
abstract = {Personalized learning research suggests that personalization may be a promising strategy for offering K-12 and Higher Education students a sense of ownership over their learning process. Studies demonstrate that personalized learning may support students’ development of new skillsets as well as scaffold lifelong learning practices, reduce time spent on learning, and mitigate information overload.&nbsp;While the research in K-12 and Higher Education is promising, research into personalized learning&nbsp;strategies&nbsp;in Workforce Training and Development Programs has been somewhat limited.&nbsp;Additionally, a literature review revealed that definitions and implementations of personalized learning vary in regard to&nbsp;what is being personalized (distribution channel, interface, content, functionality), who conducts the personalization (user,&nbsp;system, blended format), how the&nbsp;(user,&nbsp;system, blended format)&nbsp;personalizes, and&nbsp;the type of learner data collected.&nbsp;This research sought expert feedback in identifying the variables, characterizations, priorities, and applications of personalized learning in Workforce Training and Development Programs.&nbsp;Three rounds of Delphi Feedback indicated that experts conceive of personalized learning as a learner-centered strategy driven by content and social interactions and underpinned by an ethos of flexibility and learner choice.&nbsp;A 5 Level Personalized Learning Interaction Framework (PLiF) building upon Moore's Theory of Interaction is proposed to structure personalized learning for Workforce Training and Development Programs.&nbsp;&nbsp;&nbsp;},
booktitle = {Ninth International Conference on Technological Ecosystems for Enhancing Multiculturality (TEEM'21)},
pages = {501–509},
numpages = {9},
keywords = {Corporate Training, Reskilling, Retooling, Adult Education},
location = {Barcelona, Spain},
series = {TEEM'21}
}

@inproceedings{10.1145/3482632.3483068,
author = {Kong, Yanbo and Hu, Jianquan},
title = {The Incentive Mechanism of College Counselors Based on Intelligent Computing},
year = {2021},
isbn = {9781450390255},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3482632.3483068},
doi = {10.1145/3482632.3483068},
abstract = {With the development of computer technology in our country, the vigorous development of higher education and the continuous deepening of higher education system reforms, intelligent computing has been integrated into the management system of universities, and significant changes have taken place in the content and functions of university management, and new changes have been proposed. As the backbone of students' ideological and political education, the team of college counselors is an important factor to ensure the continuous, healthy and rapid development of my country's higher education. The overall level and work status of the college counselor team will directly affect the effectiveness of ideological and political education and the realization of talent training goals. This article is based on the research of the university counselor incentive system based on intelligent computing. This article first uses the literature research method to analyze some problems in the incentive system of college counselors, and then proposes the necessity of reforming the incentive system for college counselors. Finally, it uses questionnaire surveys and quantitative analysis methods to analyze the work status of college counselors and how to deal with them. Research shows that the working conditions of college counselors are not very good. About 40% of counselors feel listless in their work, which may be related to the heavy workload. College counselors are dissatisfied with the incentive system, about 45% of them. It is possible that the incentive system of colleges and universities makes counselors not have great incentives for their salary and development.},
booktitle = {2021 4th International Conference on Information Systems and Computer Aided Education},
pages = {993–997},
numpages = {5},
location = {Dalian, China},
series = {ICISCAE 2021}
}

@article{10.1145/3448974,
author = {Lalapura, Varsha S. and Amudha, J. and Satheesh, Hariramn Selvamuruga},
title = {Recurrent Neural Networks for Edge Intelligence: A Survey},
year = {2021},
issue_date = {May 2022},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {54},
number = {4},
issn = {0360-0300},
url = {https://doi.org/10.1145/3448974},
doi = {10.1145/3448974},
abstract = {Recurrent Neural Networks are ubiquitous and pervasive in many artificial intelligence applications such as speech recognition, predictive healthcare, creative art, and so on. Although they provide accurate superior solutions, they pose a massive challenge “training havoc.” Current expansion of IoT demands intelligent models to be deployed at the edge. This is precisely to handle increasing model sizes and complex network architectures. Design efforts to meet these for greater performance have had inverse effects on portability on edge devices with real-time constraints of memory, latency, and energy. This article provides a detailed insight into various compression techniques widely disseminated in the deep learning regime. They have become key in mapping powerful RNNs onto resource-constrained devices. While compression of RNNs is the main focus of the survey, it also highlights challenges encountered while training. The training procedure directly influences model performance and compression alongside. Recent advancements to overcome the training challenges with their strengths and drawbacks are discussed. In short, the survey covers the three-step process, namely, architecture selection, efficient training process, and suitable compression technique applicable to a resource-constrained environment. It is thus one of the comprehensive survey guides a developer can adapt for a time-series problem context and an RNN solution for the edge.},
journal = {ACM Comput. Surv.},
month = {may},
articleno = {91},
numpages = {38},
