booktitle = {Proceedings of the 2018 AAAI/ACM Conference on AI, Ethics, and Society},
pages = {95–101},
numpages = {7},
keywords = {transnational legal ordering, international governance, hard/soft law, international organizations},
location = {New Orleans, LA, USA},
series = {AIES '18}
}

