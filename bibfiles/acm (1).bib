address = {New York, NY, USA},
url = {https://doi.org/10.1145/3419635.3419684},
doi = {10.1145/3419635.3419684},
abstract = {The operating status of power equipment is closely related to the stability and safety of the power system. Comprehensively and accurately grasp the internal and external multi-source data of power equipment, and use scientific means to summarize and integrate the information, so as to make accurate assessment and prediction of the operating status and change trend of the equipment, and arrange a reasonable operation and maintenance plan, Is the key prerequisite and important foundation for the reliable and economic operation of the entire power system. The development and breakthrough of artificial intelligence technology (AI) in the aspects of efficiency, precision, and self-learning ability in identification, prediction, optimization, and decision-making tasks provide a new technical means and research ideas for the operation and maintenance of power equipment. The paper reviewed the key technologies of artificial intelligence in the operation and maintenance of power transmission and transformation equipment, sorted out the relevant data situation and existing problems of equipment operation and maintenance, pointed out the key technology framework of artificial intelligence and the application characteristics and scope of various algorithms. Finally, the problems faced by the application of artificial intelligence technology are analysed, and their development trends are prospected and suggested.},
booktitle = {Proceedings of the 2020 International Conference on Computers, Information Processing and Advanced Education},
pages = {182–186},
numpages = {5},
keywords = {operation and maintenance management, smart grid data, Artificial intelligence technology, data cluster analysis},
location = {Ottawa, ON, Canada},
series = {CIPAE 2020}
}

@article{10.1145/3465398,
author = {Chen, Jianguo and Li, Kenli and Zhang, Zhaolei and Li, Keqin and Yu, Philip S.},
title = {A Survey on Applications of Artificial Intelligence in Fighting Against COVID-19},
year = {2021},
issue_date = {November 2022},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {54},
number = {8},
issn = {0360-0300},
url = {https://doi.org/10.1145/3465398},
doi = {10.1145/3465398},
abstract = {The COVID-19 pandemic caused by the SARS-CoV-2 virus has spread rapidly worldwide, leading to a global outbreak. Most governments, enterprises, and scientific research institutions are participating in the COVID-19 struggle to curb the spread of the pandemic. As a powerful tool against COVID-19, artificial intelligence (AI) technologies are widely used in combating this pandemic. In this survey, we investigate the main scope and contributions of AI in combating COVID-19 from the aspects of disease detection and diagnosis, virology and pathogenesis, drug and vaccine development, and epidemic and transmission prediction. In addition, we summarize the available data and resources that can be used for AI-based COVID-19 research. Finally, the main challenges and potential directions of AI in fighting against COVID-19 are discussed. Currently, AI mainly focuses on medical image inspection, genomics, drug development, and transmission prediction, and thus AI still has great potential in this field. This survey presents medical and AI researchers with a comprehensive view of the existing and potential applications of AI technology in combating COVID-19 with the goal of inspiring researchers to continue to maximize the advantages of AI and big data to fight COVID-19.},
journal = {ACM Comput. Surv.},
month = {oct},
articleno = {158},
numpages = {32},
keywords = {Artificial intelligence, COVID-19, SARS-CoV-2}
}

