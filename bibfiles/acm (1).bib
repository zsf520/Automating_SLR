@inproceedings{10.1145/3419635.3419711,
author = {Guan, Huaiguang and Yang, Peng},
title = {Advance in Artificial Intelligence Method Safety on Warning of Tailings Dam Break},
year = {2020},
isbn = {9781450387729},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3419635.3419711},
doi = {10.1145/3419635.3419711},
abstract = {Tailing dam breaks bring big risks and often result in huge casualties and property losses. In recent years, the total number of dam break accidents in tailing ponds has declined, which fully reflects the progress of sensor technology, remote sensing technology, computer technology, especially the important role of artificial intelligence method in the early warning of break risk of tailing dams. On the basis of literature study, this paper focuses on the current situation and development of early warning of break risk of tailing dams, discussing and analyzing the problems faced by the risk warning of the tailing dam breaks, with an attempt to give suggestions for improvement so as to provide references for tailing dam disaster prevention and technical innovation. The current situation of development shows that: (1) there is a lack of relevant research of tailing ponds monitoring system, which is often designed for a single pond and has a low level of real implementations; (2) in the risk warning model of tailing dam breaks, scholars have achieved gratifying results by using machine learning, deep learning, and other prediction methods, which can to meet the requirements of data accuracy, computability, and reliability.},
booktitle = {Proceedings of the 2020 International Conference on Computers, Information Processing and Advanced Education},
pages = {229–234},
numpages = {6},
keywords = {safety monitoring, tailing dam break, artificial intelligence method, early warning},
location = {Ottawa, ON, Canada},
series = {CIPAE 2020}
}

@inproceedings{10.1145/3417188.3417197,
author = {Chen, Kun-Huang and Wang, Hui-Wu and Liu, Chung-Ming},
title = {Applying Artificial Intelligence to Survival Prediction of Hepatocellular Carcinoma Patients},
year = {2020},
isbn = {9781450375481},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3417188.3417197},
doi = {10.1145/3417188.3417197},
abstract = {Cancer, a disease that has gradually attracted attention in the world now, it has even become one of the main causes of death. Among them, liver cancer occurs in the liver or deadly tumors starting from the liver. According to the World Cancer Report (2014), the liver cancer is primary the cancer that the occurrence of 6% ranked second highest, and the death rate is 9% ranked sixth. So the liver cancer has become the target of academic research and discussion. If we can find out the key factors that affect the death of liver cancer when identifying liver cancer, it can improve the survival prediction of patients with liver cancer, and it will bring more effective treatment and confidence to the disease. In this paper, we chose a data that a real clinically diagnosed HCC patient was collected from a University of Coimbra and Coimbra Hospital in Portugal, and we separated the data into testing and training to predict the death of HCC and find out the key factors from the prediction model. The prediction model includes Decision Tree (DT), Support vector machine (SVM), and Logistic Regression (LR). The results of this paper showed that the G-means of the three modeling methods are 0.76 (LR), 0.72 (DT), and 0.68 (SVM). The best performance is logistic regression (LR), and find out the key factors that affect the survival rate of HCC include Aspartate transaminase (U / L), Age at diagnosis, and Alkaline phosphatase (U / L).},
booktitle = {Proceedings of the 2020 4th International Conference on Deep Learning Technologies},
pages = {135–139},
numpages = {5},
keywords = {Logistic Regression, liver cancer, Support vector machine, Decision Tree, HCC},
location = {Beijing, China},
series = {ICDLT '20}
}

@inproceedings{10.1145/3545822.3545831,
author = {Lu, Wenjie},
title = {On the Mechanism of Artificial Intelligence Affecting Sports Tourism Industry},
year = {2022},
isbn = {9781450396424},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3545822.3545831},
doi = {10.1145/3545822.3545831},
abstract = {Using the research methods of literature, induction and mathematical statistics, under the background that AI cuts into various industrial fields and has the dual characteristics of technology and capital, this paper constructs the action mechanism model of AI affecting sports tourism industry by combing the relevant theories of AI affecting sports tourism industry. The results show that the progress of AI technology has an advanced quantitative and qualitative effect on sports tourism industry; The impact of AI on sports tourism industry is mainly realized through three action mechanisms. At the same time, the three action mechanisms are empirically tested respectively. The purpose is to further verify the development role of AI technology in the advanced stage of sports tourism industry from the theoretical and practical levels, so as to promote the sustainable development of sports tourism industry.},
booktitle = {Proceedings of the 2022 7th International Conference on Multimedia Systems and Signal Processing},
pages = {57–62},
