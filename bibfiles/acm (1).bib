@inproceedings{10.1145/3154943.3154950,
author = {Cremer, Stefan and Ma, Alice Kyungran},
title = {Predicting E-Commerce Sales of Hedonic Information Goods via Artificial Intelligence Imagery Analysis of Thumbnails},
year = {2017},
isbn = {9781450353120},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3154943.3154950},
doi = {10.1145/3154943.3154950},
abstract = {In e-commerce, thumbnail images serve as concise visual summaries of complex hedonic information goods, such as books, movies or games. Despite thumbnails' omnipresence and their assumed effect on sales, they largely constitute a black box for research and practice. This work aims to fill this gap and asks, with regard to hedonic information goods: What is the role of thumbnails in the purchase process and how and why do their basic, conceptual and social features impact sales? To answer the question, we first analytically and empirically show that thumbnails play a more central role in the purchase process for hedonic information goods than for other types of goods. Next, we develop a variance model on how and why thumbnails affect sales. Using artificial intelligence imagery analysis, we test the model against evidence from sales data of 100,000 hedonic information goods from a large e-commerce platform. Our findings entail theoretical contributions to the growing field of consumers' visual information processing in e-commerce contexts and lead us to implications for managerial practice.},
booktitle = {Proceedings of the International Conference on Electronic Commerce},
articleno = {5},
numpages = {8},
keywords = {artificial intelligence imagery analysis, visual perception, hedonic information goods, consumer choice},
location = {Pangyo, Seongnam, Republic of Korea},
series = {ICEC '17}
}

@inproceedings{10.1145/3560107.3560110,
author = {Medaglia, Rony and Tangi, Luca},
