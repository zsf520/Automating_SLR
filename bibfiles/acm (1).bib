title = {Research Hotspots and Trends of Educational Application of Artificial Intelligence at Home and Abroad in the Past Ten Years – Visual Analysis Based on CiteSpace},
year = {2022},
isbn = {9781450396455},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3551708.3551719},
doi = {10.1145/3551708.3551719},
abstract = {With the development of “artificial intelligence plus education”, the application of artificial intelligence(AI) in education has become a research hotspot that many researchers pay attention to. In order to explore the research status of education applications of artificial intelligence (EDA-AI) at home and abroad in the past ten years, this paper uses CiteSpace to analyze 346 papers retrieved from China National Knowledge Infrastructure (CNKI) and 235 papers retrieved from Web of Science (WOS). The study found that: (1) The research on EDA-AI at home and abroad is developing, and the number of published papers has surged since 2017 in China and 2019 in foreign respectively; (2) Foreign institutions and scholars have closer cooperative relations and more frequent exchanges than domestic institutions and scholars; (3) The research content focuses on the application of AI technology and AI products in education. Among them, domestic researches were more focused on theoretical exploration and elaboration, while foreign researches were more focused on practical application and AI system development. Finally, in order to provide reference for the application of AI in education, this paper puts forward prospects from four aspects: interdisciplinary research, ethics research, educational application scenarios and forms, and research and development of AI products in education.},
booktitle = {Proceedings of the 6th International Conference on Education and Multimedia Technology},
pages = {123–130},
numpages = {8},
keywords = {CiteSpace Software, Educational Applications, Research Hotspots, Research Trends, AI, Artificial Intelligence},
location = {Guangzhou, China},
series = {ICEMT '22}
}

@inproceedings{10.1145/3528137.3528162,
author = {Chen, Qinxian and Xing, Saipeng},
