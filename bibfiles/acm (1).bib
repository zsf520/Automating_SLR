year = {2022},
isbn = {9781450396271},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3528137.3528162},
doi = {10.1145/3528137.3528162},
abstract = {Under the background of artificial intelligence, the internal and external environment of enterprises has changed dramatically, the market environment presents the characteristics of dynamic, complexity and competitiveness, the organizational structure has changed, and the team has increasingly become a common working mode in enterprises. As an important ability to generate novel and useful ideas, team creativity is an important source of organizational innovation and competitive advantage. The relationship between leadership style and team creativity has always been a hot topic in the field of organizational behavior. How to stimulate team creativity is still an important problem to be solved in the innovation and development of enterprises. This paper takes the R &amp; D team of scientific and technological enterprises as the research object, takes "input-process-output" (I-P-O, as an abbreviation of input-processing-output) as the analysis paradigm, takes the leadership empowerment behavior as the antecedent variable, the team interaction process as the intermediary variable, and the team creativity as the outcome variable, This paper puts forward the transmission path of "leadership empowerment behavior - team interaction process - team creativity", and verifies the intermediary role of team interaction process between leadership empowerment behavior and team creativity.},
booktitle = {Proceedings of the 2022 3rd International Conference on Education Development and Studies},
pages = {146–151},
numpages = {6},
keywords = {Team creativity, Team interaction process, Leadership empowerment behavior, Key Words: Artificial intelligence},
location = {Hilo, HI, USA},
series = {ICEDS '22}
}

@inproceedings{10.1145/3508259.3508295,
author = {Sasmoko, Given Name and Indrianti, Yasinta and Ronald Hermanus, Davy},
title = {LINEN ASSESSMENT APPS: Artificial Intelligence Integration to Develop Android-Based Applications That Measure the "Literacy, Numeracy, Entrepreneurial Mindset" Capacity of Students in Indonesia},
year = {2022},
