@inproceedings{10.1145/3524383.3524405,
author = {Liu, Xiaolin and Li, Yaoqing},
title = {Redefining Teacher Qualification in the Artificial Intelligence Era: A Professional Capital Perspective},
year = {2022},
isbn = {9781450395793},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi.org/10.1145/3524383.3524405},
doi = {10.1145/3524383.3524405},
abstract = {Artificial Intelligence (AI) is challenging teacher professionalism, thus how to define a teacher in the AI era has become a hot topic in education. Many studies adopted a teacher literacy perspective on redefining a teacher for artificial intelligence in education(AIEd), while ignoring the sociological observation of the challenges faced with teachers in their professional field. This paper was framed by the core concept “professional capital” from sociology, the revolutionary power in the field, analyzed the trend of education field evolving from traditional education to AIEd. Based on the analysis, the paper proposed that, teacher professional capital is the impetus that empowers teachers to reconstruct their professional identity in the AI era. Teacher professional capital for AIEd mainly manifests itself in three forms: teacher human capital(THC), representing teacher agency to teach with AI at the individual level; teacher social capital(TSC), representing the synergetic resources circulating in teachers’ social networks at the interpersonal system; teacher decisional capital(TDC), representing the collaborative decision-making power at the human-AI level. In teachers’ professional practice, THC empowers teachers the capability to transit from traditional education field to AIEd field; TSC endows teachers the power to leverage other people's professional capital to teach with AI; TDC ensures teachers professional autonomy to teach with AI in a reasonable and legal manner.},
booktitle = {Proceedings of the 5th International Conference on Big Data and Education},
pages = {35–39},
numpages = {5},
keywords = {Education Field, Pedagogy Innovation, Teacher Professional Development, Teacher Professional Capital, Artificial Intelligence in Education},
location = {Shanghai, China},
series = {ICBDE '22}
}

