@inproceedings{10.1145/3486011.3486514,
author = {Gonz\'{a}lez Arrieta, Ang\'{e}lica and Garc\'{\i}a-Bermejo-Giner, Jos\'{e} R. Rafael and P\'{e}rez L\'{a}ncho, Bel\'{e}n and Mart\'{\i}n Garc\'{\i}a, Teresa and \'{A}lvarez Navia, Iv\'{a}n and Hern\'{a}ndez Sim\'{o}n, Juan Andr\'{e}s and S\'{a}nchez L\'{a}zaro, \'{A}ngel Luis and Vega Cruz, Pastora Isabel},
