abstract = {Weifang is an emerging electronic-information-industry city in China. The fast development of local industries raise huge demands on application-oriented technical talents of various fields, among which-artificial intelligence (AI) takes the leading position. However, it is a challenging task to train AI talents due to the complexity of AI. We investigate on training AI talents in application-oriented universities like Weifang University. We propose to integrate theoretical knowledge and hands-on ability to support the growth of comprehensive ability. Subsequently, we factorize the knowledge and ability structure into small pieces to lower the learning barrier. Based on these two solutions, we try to cultivate the engineering thinking and ability of students. Our practice in technical competition and research projects show the effectiveness of the solutions.},
booktitle = {2021 2nd International Conference on Computers, Information Processing and Advanced Education},
pages = {602–605},
numpages = {4},
