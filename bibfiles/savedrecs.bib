@article{ WOS:000870341900082,
Author = {Chen, Liang-Bi and Liu, Yi-Hsuan and Huang, Xiang-Rui and Chen, Wei-Han
   and Wang, Wei-Chien},
Title = {Design and Implementation of a Smart Seawater Aquarium System Based on
   Artificial Intelligence of Things Technology},
Journal = {IEEE SENSORS JOURNAL},
Year = {2022},
Volume = {22},
Number = {20},
Pages = {19908-19918},
Month = {OCT 15},
Abstract = {In recent years, single societies and aging societies around the
   worldwide increasingly seen the emerging problem of loneliness and the
   growing need for companionship. To satisfy the need, the population of
   pet keepers is growing, and the proportion of households with pets is
   increasing. It is normal to have pets at home. Pet fish are kept not
   only for companionship but also to relieve the stress accumulated
   throughout the day, and they can also improve the quality of life and
   health of older individuals. Having a pet can help reduce anxiety and
   increase food intake in people with medical conditions. This article
   proposes a smart seawater aquarium system based on the artificial
   intelligence of things (AIoT) technology to lower the knowledge
   threshold for keeping seahorses or other marine fish as pets. The
   proposed system provides seawater fish owners with safe and convenient
   feeding methods and more pet-friendly and user-friendly choices of
   interactive pet care and improves the comfort and survival rate of
   marine life in seawater aquariums. This smart seawater aquarium system
   can automatically or manually adjust water quality, water temperature,
   and oxygen content in water, change the water, feed fish, and monitor
   underwater fish in real time. It solves the problem of neglecting pet
   fish and the difficulty of maintaining seawater aquariums. Moreover, the
   proposed system incorporates a cloud-based information system and a
   web-based platform for data transmission and interface control, and it
   has achieved the purposes of remote and local monitoring of the
   aquarium.},
