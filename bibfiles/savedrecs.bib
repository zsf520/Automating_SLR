@article{ WOS:000855606300003,
Author = {Ravichandran, Bhavani Devi and Keikhosrokiani, Pantea},
Title = {Classification of Covid-19 misinformation on social media based on
   neuro-fuzzy and neural network: A systematic review},
Journal = {NEURAL COMPUTING \& APPLICATIONS},
