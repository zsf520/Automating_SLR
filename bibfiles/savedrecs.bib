@article{ WOS:001090974200001,
Author = {Marzia, Syeda and Vital-Soto, Alejandro and Azab, Ahmed},
Title = {Automated Process Planning and Dynamic Scheduling for Smart
   Manufacturing: A Systematic Literature Review},
