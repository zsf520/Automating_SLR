@article{ WOS:001050981500001,
Author = {Lu, Yingqi and Pei, Weidi and Peng, Kaiyuan},
Title = {State of the art of automatic disassembly of WEEE and perspective
   towards intelligent recycling in the era of Industry 4.0},
