@article{ WOS:000610613700004,
Author = {Wang, Dee Dee and Qian, Zhen and Vukicevic, Marija and Engelhardt, Sandy
   and Kheradvar, Arash and Zhang, Chuck and Little, Stephen H. and
   Verjans, Johan and Comaniciu, Dorin and O'Neill, William W. and Vannan,
   Mani A.},
Title = {3D Printing, Computational Modeling, and Artificial Intelligence for
   Structural Heart Disease},
Journal = {JACC-CARDIOVASCULAR IMAGING},
