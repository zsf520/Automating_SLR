@article{ WOS:001032089000003,
Author = {Zhang, Yichan and Duan, Wentao and Villanueva, Lizelle E. and Chen,
   Sheng},
Title = {Transforming sports training through the integration of internet
   technology and artificial intelligence},
Journal = {SOFT COMPUTING},
Year = {2023},
Volume = {27},
Number = {20},
Pages = {15409-15423},
Month = {OCT},
Abstract = {The Internet of Things (IoT) has gained significant popularity through
   wearable technologies. Several IoT devices are used in educational
   institutions, smart homes, and security management. Their widespread use
   in sports training applications using IoT (WS-IoT)-based wearable
   sensors is noteworthy. Meanwhile, artificial intelligence (AI) has
   evolved rapidly, and its applications have expanded across many diverse
   fields. Over the past few years, China has placed greater emphasis on
   physical education to enhance students' physical fitness and overall
   capabilities. A significant number of colleges and universities have
   adopted artificial intelligence to transform traditional single-mode
   sports training methods. This paper introduces a continuous monitoring
   system for sports individuals using the WS-IoT. The goal of coaches is
   to maximize the results of athlete training across various sports events
   using AI technology and their sports expertise. In this study, we
   examine the use of AI technology in sports and assess the practical
   implications of its implementation. To achieve the objective function,
   we used a Bayesian deep classifier and deep learning algorithms that
   utilize adaptive optimization. Utilizing artificial intelligence in
   Chinese educational institutions improves training efficiency and
   effectiveness. The system was then rigorously tested and analyzed in
   terms of the quality-of-service parameters, including communication
   collisions and packet loss rates. It is shown that packet loss rates
   increase with increasing packet numbers, but collision rates remain
   unchanged. Furthermore, a practical application of the system in
   colleges and universities showed that 14.4\% of the students were
   extremely satisfied with the use of teaching materials, 24.5\% were
   satisfied with the materials, 30.1\% were satisfied with the training
   methods, and 17.9\% were dissatisfied.},
