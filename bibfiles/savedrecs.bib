@article{ WOS:000841086900001,
Author = {Jyothi, Parvathy and Singh, A. Robert},
Title = {Deep learning models and traditional automated techniques for brain
   tumor segmentation in MRI: a review},
Journal = {ARTIFICIAL INTELLIGENCE REVIEW},
