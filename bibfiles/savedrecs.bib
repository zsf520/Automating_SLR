@article{ WOS:000639640500004,
Author = {Jin, Gao},
Title = {Application of load balancing image processing system and artificial
   intelligence data in college dance teaching mode},
Journal = {JOURNAL OF AMBIENT INTELLIGENCE AND HUMANIZED COMPUTING},
Year = {2021},
Month = {2021 APR 12},
Abstract = {Computer has become the main part of teaching work. Arranging courses
   through the computer's intelligent control system is also a complex and
   heavy project in computer system engineering. The load balancing image
   processing system is that the user sends the request to the load
   balancer, and uses the scheduling algorithm to send the user request to
   the back-end server. The back-end server processes the user request in a
   timely manner, and feeds back the processing result to the load
   balancer, and the load balancer finally feeds back the processing result
