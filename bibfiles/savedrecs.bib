@article{ WOS:000574777500003,
Author = {Nassour, J. and Leykin, D. and Elhadad, M. and Cohen, O.},
Title = {Computational Text Analysis of A Scientific Resilience Management
   Corpus: Environmental Insights and Implications},
Journal = {JOURNAL OF ENVIRONMENTAL INFORMATICS},
