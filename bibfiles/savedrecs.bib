@article{ WOS:000618076700163,
Author = {Wang, Yuxia},
Title = {Ideological and political teaching model using fuzzy analytic hierarchy
   process based on machine learning and artificial intelligence},
Journal = {JOURNAL OF INTELLIGENT \& FUZZY SYSTEMS},
