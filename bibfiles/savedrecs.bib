@article{ WOS:000787268100001,
Author = {Abdelwahab, Hanna Rahma and Rauf, Abdul and Chen, Dadi},
Title = {Business students' perceptions of Dutch higher educational institutions
   in preparing them for artificial intelligence work environments},
Journal = {INDUSTRY AND HIGHER EDUCATION},
Year = {2023},
Volume = {37},
Number = {1},
Pages = {22-34},
Month = {FEB},
Abstract = {The application of artificial intelligence (Al) brings new demand to the
   job market. The concern now is whether higher education institutions
   (HEIs) have adequately prepared young learners to work in an Al work
   environment. This research seeks to explore business students'
   perceptions of their HEIs in the Netherlands in preparing them for Al
   work environments. A questionnaire-based survey was completed by 95
   students from 27 HEIs in the Netherlands. The findings show that these
   students believe that their HEIs are not optimally equipped at this time
   and/or have not optimally utilized their facilities to adequately
   prepare them for Al work environments. This study reinforces the urgency
   of updating the curriculum and educational facilities for Al work
   environments and provides suggestions for future research.},
