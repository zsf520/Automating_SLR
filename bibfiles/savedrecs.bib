@article{ WOS:000710092800001,
Author = {Burggraef, Peter and Adlon, Tobias and Hahn, Viviane and
   Schulz-Isenbeck, Timm},
Title = {Fields of action towards automated facility layout design and
   optimization in factory planning - A systematic literature review},
Journal = {CIRP JOURNAL OF MANUFACTURING SCIENCE AND TECHNOLOGY},
Year = {2021},
Volume = {35},
Month = {NOV},
Abstract = {In order to develop and establish a functional, integrated system for
   automated layout design that can be applied in practice, certain fields
   of action still need to be identified and solved. For this purpose, a
   systematic literature review is used to identify fields of action on the
   way to automated factory layout design and to derive recommendations for
   action. As a result, the need for action was identified in the areas of
   multicriteria optimization, the layout evaluation and selection process,
   the existing implemented algorithms and in the integration of the human
   planner. To make qualitative target criteria machine-interpretable was
   identified as the most important next step. (C) 2021 The Authors.},
