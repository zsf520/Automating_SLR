@article{ WOS:001017201400001,
Author = {Butt, Shimza and Khan, Saif Ur Rehman and Hussain, Shahid and Wang, Wen
   -Li},
Title = {A conceptual model supporting decision-making for test automation in
   Agile-based Software Development},
Journal = {DATA \& KNOWLEDGE ENGINEERING},
