@article{ WOS:000867245400001,
Author = {Dube, Tinyiko Vivian and Jacobs, Lorette},
Title = {Academic library services extension during the COVID-19 pandemic:
   considerations in higher education institutions in the Gauteng Province,
   South Africa},
