@article{ WOS:000981247100001,
Author = {Hwang, Gwo-Jen and Chen, Nian-Shing},
Title = {Editorial Position Paper: Exploring the Potential of Generative
   Artificial Intelligence in Education: Applications, Challenges, and
   Future Research Directions},
