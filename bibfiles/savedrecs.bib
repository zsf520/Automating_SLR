@article{ WOS:000483130800007,
Author = {von Chamier, Lucas and Laine, Romain F. and Henriques, Ricardo},
Title = {Artificial intelligence for microscopy: what you should know},
Journal = {BIOCHEMICAL SOCIETY TRANSACTIONS},
