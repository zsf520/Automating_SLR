@article{ WOS:000682240500001,
Author = {Vazquez-Verdera, Victoria and Domingo, Juan and Dura, Esther and
   Gabaldon-Estevan, Daniel and Lopez-Baeza, Ernesto and Machause Lopez,
   Sonia and Meco-Tebar, Fabiola and Rueda, Silvia and Serrano-Lara, Jose
   Javier and Signes-Soler, Isabel and Vazquez de agredos Pascual, Maria
   Luisa and Martinez-Garcia, Elena},
Title = {The Future We Want: A Learning Experience to Promote SDGs in Higher
   Education from the United Nations and University of Valencia},
Journal = {SUSTAINABILITY},
Year = {2021},
Volume = {13},
Number = {15},
Month = {AUG},
Abstract = {This article shares the strategy for mainstreaming the Sustainable
   Development Goals (SDGs) at the University of Valencia (UV), which,
   although limited in its scale, may compel other Higher Education
   Institutions to think in technological and social progress aligned with
   the 2030 Agenda. It explicates a process driven by the UV, on the
   occasion of the 75th anniversary of the United Nations (UN), and in
   collaboration with the Service for Geospatial, Information, and
