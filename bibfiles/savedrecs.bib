@article{ WOS:001050843100001,
Author = {Alqahtani, Tariq and Badreldin, Hisham A. and Alrashed, Mohammed and
   Alshaya, Abdulrahman I. and Alghamdi, Sahar S. and bin Saleh, Khalid and
   Alowais, Shuroug A. and Alshaya, Omar A. and Rahman, Ishrat and Al Yami,
   Majed S. and Albekairy, Abdulkareem M.},
Title = {The emergent role of artificial intelligence, natural learning
   processing, and large language models in higher education and research},
