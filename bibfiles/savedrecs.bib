@article{ WOS:000636232500011,
Author = {Zeng, Zhen and Qi, Longqi},
Title = {``Internet plus Artificial Intelligence{''} Human Resource Information
   Management System Construction Innovation and Research},
Journal = {MATHEMATICAL PROBLEMS IN ENGINEERING},
