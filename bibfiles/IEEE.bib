  month={June},}@INPROCEEDINGS{9784630,
  author={Kaushik, Rachna and Parmar, Mamta and Jhamb, Shelja},
  booktitle={2021 2nd International Conference on Computational Methods in Science & Technology (ICCMST)}, 
  title={Roles and Research Trends of Artificial Intelligence in Mathematics Education}, 
  year={2021},
  volume={},
  number={},
  pages={202-205},
  abstract={For many pupils, learning mathematics has been a significant problem. The growth of computer technology, particularly artificial intelligence (AI), offers a solution to this problem by detecting individual students' learning challenges and giving individualized support to help them perform better in mathematics classes. However, there is a dearth of evaluations from a variety of viewpoints to assist researchers, particularly novices, in gaining a comprehensive understanding of AI research in mathematics education. To that end, this study will conduct a bibliometric mapping analysis and systematic review to investigate the role and research trends of AI in mathematics education by searching the Web of Science (WOS) database for relevant articles published in high-quality journals indexed by the Social Sciences Citation Index (SSCI). Furthermore, by referring to the technology-based learning model, several aspects of AI in mathematics education research are taken into account, including application domains, participants, research methods, adopted technologies, research issues and AI roles, as well as citation and co-citation relationships. As a result, progress in AI research in mathematics education is presented, and possible study subjects for future research are suggested.},
  keywords={},
