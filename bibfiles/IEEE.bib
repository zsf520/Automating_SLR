  abstract={Under the background of "informatization teaching, the online and offline hybrid teaching mode is bringing subversive changes to traditional higher education. This article takes the professional course "Video Image Processing and Interpretation" as an example, and aims at the particularity of the noncommissioned student group, proposed a hybrid teaching model suitable for noncommissioned officers’ vocational education professional courses, and adjusted the previous hybrid teaching model of "online and offline integration", aiming to cultivate "ability" instead of "written knowledge" to improve the quality of talent training.},
  keywords={},
  doi={10.1109/ICAIE53562.2021.00132},
  ISSN={},
  month={June},}@INPROCEEDINGS{9569548,
  author={Cheddadi, Saoussen and Bouache, Mourad},
